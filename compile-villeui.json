{
  "$schema": "https://qooxdoo.org/schema/compile-1-0-0.json",
  "targets": [
    {
      "type": "source",
      "outputPath": "compiled/source"
    },
    {
      "type": "build",
      "outputPath": "compiled/build"
    }
  ],
  "defaultTarget": "build",
  "locales": ["en"],
  "environment": {
      "ville.cssuc": true,
      "qx.dyntheme": false,
      "qx.nativeScrollBars": true
  },
  "babel" : {
    "options" : {
      "targets" : "> 0.25%, not dead"
    }
  },
  "applications": [
    {
      "class": "villeui.Application",
      "theme": "ville.cssuc.theme.blankslate.Theme",
      "name": "villeui",
      "bootPath": "source/boot/villeui",
      "include": ["ville.cssuc.*"],
      "parts": {
        "boot": {
            "include": [
                "villeui.Application",
                "ville.cssuc.*"
            ]
        },
        "WidgetBrowser": {
            "include": [
                "villeui.WidgetBrowser"
            ]
        },
        "NativeCss": {
            "include": [
                "villeui.NativeCss"
            ]
        }
      }
    }
  ]
}
